{
  "name": "vetra-ui",
  "version": "v1",
  "private": true,
  "scripts": {
    "dev": "pnpm run check-license && next dev",
    "build": "next build",
    "check-license": "node scripts/check-license.js",
    "prebuild": "node scripts/check-license.js",
    "start": "pnpm run check-license && serve -s out",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "verify": "pnpm run check-license && pnpm run typecheck && pnpm run lint"
  },
  "keywords": [
    "nextjs",
    "landing-page",
    "template",
    "tailwindcss",
    "typescript",
    "glassmorphism"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kmucdigital/vetra-ui"
  },
  "bugs": {
    "url": "https://github.com/kmucdigital/vetra-ui/issues"
  },
  "homepage": "https://github.com/kmucdigital/vetra-ui#readme",
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "kmuc-dev-cli": "^2.2.1",
    "lucide-react": "^0.468.0",
    "next": "^15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-next": "^15.1.3",
    "postcss": "^8.4.49",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  }
